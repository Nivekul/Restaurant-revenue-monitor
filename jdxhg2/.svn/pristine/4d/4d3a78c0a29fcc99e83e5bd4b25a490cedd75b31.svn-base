body {
  height: 100%;
  background-color: rgb(28,27,32);
}

.wrapper {
  margin: 4%;
  border-radius: 8px;
  box-shadow: 0 0 10px 0px rgba(0, 0, 0, 0.5);
  background-color: white;
  overflow: hidden;
}

.header-pic {
  width: 100%;
}

.form-header {
  height: 20%;
  overflow: hidden;
}

.wrapper-header {

}

.wrapper-form {
  padding: 5px 20px 5px 20px;
}

.wrapper-submit {
  margin-top: auto;
}

select {
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
}

input {
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
}

.fa {
  width: 12px;
}

.input-group-addon {
  background-color: white;
}

.form-control {
  box-shadow: none;
  -webkit-box-shadow: none;
}

.submit {
  padding: 14px 20px;
  border-radius: 0px;
  border-color: transparent;
}
